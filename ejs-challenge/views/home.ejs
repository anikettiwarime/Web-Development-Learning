<%- include("partials/header") -%>
    <h1>Home Page</h1>
    <p>
        <%=StartingContent %>
    </p>
    <p>
        <% posts.forEach(function(post){ %>
            <h1>
                <%=post.title %>
            </h1>
            <p>
                <%=post.content.substring(0,100)+"..." %>
                <a href="posts/<%=post.title %>">Read More</a>
            </p>
            <% }) %>
    </p>
    <%- include("partials/footer") -%>